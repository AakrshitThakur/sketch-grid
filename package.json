{
  "name": "sketch-grid",
  "private": true,
  "description": "Sketch Grid is a collaborative whiteboard for creating visual elements that help teams work more efficiently",
  "scripts": {
    "init:tsc": "npx tsc --init",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nanoid": "^2.1.0",
    "@types/react-toastify": "^4.0.2",
    "autoprefixer": "^10.4.21",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.1.6",
    "postcss": "^8.5.6",
    "prisma": "^6.17.1",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
    "tailwindcss": "^4.1.16",
    "zod": "^4.1.12"
  }
}
